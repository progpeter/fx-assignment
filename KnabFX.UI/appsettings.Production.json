{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ExternalServices": {
    "ExchangeRatesServiceConfig": {
      "BaseAddress": "https://api.apilayer.com/",
      "DefaultApi": "/exchangerates_data/latest",
      "Headers": {
        "Accept": "application/json",
        "apiKey": "<ExchangeRates_APIKey>"
      }
    },
    "CryptoRatesServiceConfig": {
      "BaseAddress": "https://pro-api.coinmarketcap.com/",
      "DefaultApi": "/v2/tools/price-conversion",
      "Headers": {
        "Accept": "application/json",
        "X-CMC_PRO_API_KEY": "<CryptoRates_LiveAPIKey>"
      }
    }
  },
  "Caching": {
    "DefaultTTL": 600
  },
  "Currency": {
    "BaseCurrency": "USD",
    "ConvertableCurrencies": "EUR,GBP,AUD,BRL"
  }
}

